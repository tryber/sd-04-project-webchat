{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "node": true,
        "jest": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"],
          "extensions": [".js", ".jsx"]
        }
      }
    },
    "extends": "airbnb-base",
    "parserOptions": { "ecmaVersion": 11 },
    "rules": {
      "no-console": "off",
      "no-alert": "off",
      "arrow-parens": [2, "always"],
      "jsx-a11y/label-has-associated-control": 0,
      "import/no-unresolved": "off",
      "quotes": [2, "single"],
      "implicit-arrow-linebreak": "off",
      "consistent-return": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ],
      "object-curly-newline": "off"
    }
  }
  